class QuartalsumsatzEnhanced {

	public static void main(String... args) {
		int anzZeilen = 3, anzSpalten = 4;
		int[][] umsatz = new int[anzZeilen][anzSpalten];
		for(int i: umsatz) { 
			for(int j: umsatz) {
				umsatz[i][j] = (i + j) * 1000;
			}
		}


		for(int i: umsatz) {
			for(int j: umsatz) {
				System.out.println("Filiale " + i + " hat im Quartal " + 
					(j+1) + " folgenden Umsatz: " + umsatz[i][j]);	
	
			}
			System.out.println();

		}

	}

}



