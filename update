#! /bin/bash

echo -e "\nUPDATEING!\n"
sudo apt update;
echo -e "\nUPDATE erledigt!\n"

echo -e "\nUPGRADING!\n"
sudo apt upgrade;
echo -e "\nUPGRADE erledigt!\n"

echo -e "\nDISTing!\n"
sudo apt dist-upgrade;
echo -e "\nDIST-UPGRADE erledigt!\n"

echo -e "\nAufr√§umen...\n"
sudo apt autoremove
echo -e "\nAUTOREMOVE erledigt!\n"






